<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name = "org.jsp.AssignmentApp.Employee" table="Employee2">
		<id name ="id" column="id">
			<generator class="identity" />
		</id>
	<property name="name" column ="name" not-null="true" />
	<property name="desg" column="desg" not-null="true" />
	<property name="salary" column="salary" not-null="true" />
	<property name="phone" column="phone" not-null="true" unique="true" />
	<property name="password" column="password" not-null="true"></property>
	</class>
</hibernate-mapping>